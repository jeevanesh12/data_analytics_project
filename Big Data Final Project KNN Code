   "source": [
    "# Cell 5: Construct a single row per match from the HOME team's perspective\n",
    "\n",
    "# Split into home and away rows\n",
    "home_df = df[df[\"venue\"] == \"Home\"].copy()\n",
    "away_df = df[df[\"venue\"] == \"Away\"].copy()\n",
    "\n",
    "# Prepare away-team form to merge onto home rows\n",
    "away_form = away_df[[\"season\", \"date\", \"time\", \"team_key\", \"opp_key\"] + rolling_cols].copy()\n",
    "\n",
    "# Flip keys so they line up with home (team_key = home team, opp_key = away team)\n",
    "away_form = away_form.rename(columns={\"team_key\": \"opp_key\", \"opp_key\": \"team_key\"})\n",
    "\n",
    "# Rename away rolling columns\n",
    "away_feature_rename = {col: \"away_\" + col for col in rolling_cols}\n",
    "away_form = away_form.rename(columns=away_feature_rename)\n",
    "\n",
    "# Home form + result (target)\n",
    "home_form = home_df[[\"season\", \"date\", \"time\", \"team_key\", \"opp_key\", \"result\"] + rolling_cols].copy()\n",
    "\n",
    "# Merge home & away forms\n",
    "merged = home_form.merge(\n",
    "    away_form,\n",
    "    on=[\"season\", \"date\", \"time\", \"team_key\", \"opp_key\"],\n",
    "    how=\"inner\"\n",
    ")\n",
    "\n",
    "# Map result to 3-class target: Home / Draw / Away\n",
    "merged[\"target\"] = merged[\"result\"].map({\"W\": \"Home\", \"D\": \"Draw\", \"L\": \"Away\"})\n",
    "\n",
    "X_cols = [c for c in merged.columns if \"rolling_mean_5\" in c or \"away_\" in c]\n",
    "data = merged.dropna(subset=X_cols + [\"target\"]).copy()\n",
    "\n",
    "print(\"Final dataset shape:\", data.shape)\n",
    "print(data[\"target\"].value_counts())\n",
    "data.head()\n"
   ]
  },
