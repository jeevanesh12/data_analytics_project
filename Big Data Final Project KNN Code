   "source": [
    "# Cell 7: Tune K using 5-fold cross-validation\n",
    "\n",
    "k_values = list(range(1, 51, 2))  # odd K values to reduce ties\n",
    "cv_scores = []\n",
    "\n",
    "for k in k_values:\n",
    "    pipe = Pipeline([\n",
    "        (\"scaler\", StandardScaler()),\n",
    "        (\"knn\", KNeighborsClassifier(n_neighbors=k))\n",
    "    ])\n",
    "    scores = cross_val_score(pipe, X_train, y_train, cv=5, scoring=\"accuracy\")\n",
    "    cv_scores.append(scores.mean())\n",
    "\n",
    "best_idx = int(np.argmax(cv_scores))\n",
    "best_k = k_values[best_idx]\n",
    "\n",
    "print(\"Best K from CV:\", best_k)\n",
    "print(\"Best CV accuracy:\", round(cv_scores[best_idx], 3))\n"
   ]
  },
