   "source": [
    "# Cell 4: Build last-5-game rolling average features (pre-match form)\n",
    "\n",
    "stats_cols = [\"gf\", \"ga\", \"xg\", \"xga\", \"poss\", \"sh\", \"sot\"]\n",
    "\n",
    "# Sort so rolling windows follow chronological order per team and season\n",
    "df = df.sort_values([\"season\", \"team_key\", \"datetime\"]).reset_index(drop=True)\n",
    "\n",
    "for col in stats_cols:\n",
    "    feat_name = f\"{col}_rolling_mean_5\"\n",
    "    df[feat_name] = (\n",
    "        df.groupby([\"season\", \"team_key\"])[col]\n",
    "          .rolling(window=5, min_periods=1)\n",
    "          .mean()\n",
    "          .shift(1)  # shift so only past games are used\n",
    "          .reset_index(level=[0, 1], drop=True)\n",
    "    )\n",
    "\n",
    "rolling_cols = [c for c in df.columns if \"rolling_mean_5\" in c]\n",
    "df[[\"team_key\", \"season\"] + rolling_cols]\n"
   ]
  },
